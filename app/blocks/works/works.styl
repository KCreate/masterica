.works
	position relative
	background $color-blue
	&:before,
	&:after
		position absolute
		content "" 
	&:before
		top -25px
		left 0
		width 100%
		height 25px 
		background url("../images/our-work-top.png") 0 0 repeat-x 
	&__in
		margin auto
		padding-top 31px
		padding-bottom 45px
		width 1005px
		text-align center
	&__title
		position relative
		margin-bottom 29px
		@extend .title
		color $color-white 
		

